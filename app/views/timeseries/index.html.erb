<p id="notice"><%= notice %></p>

<h1>Timeseries</h1>

<table>
  <thead>
    <tr>
      <th>Integer</th>
      <th>Datetime</th>
      <th>Double</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timeseries.each do |timeseries| %>
      <tr>
        <td><%= timeseries.integer %></td>
        <td><%= timeseries.datetime %></td>
        <td><%= timeseries.double %></td>
        <td><%= link_to 'Show', timeseries %></td>
        <td><%= link_to 'Edit', edit_timeseries_path(timeseries) %></td>
        <td><%= link_to 'Destroy', timeseries, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timeseries', new_timeseries_path %>
